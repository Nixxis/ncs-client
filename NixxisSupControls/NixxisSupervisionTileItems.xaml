    <!-- Alert template -->
    <Style x:Key="NixxisSupervisionTileAlertStyle" TargetType="{x:Type local:NixxisSupervisionTileAlert}">
        <!--Big Template-->
        <Setter Property="LargeTemplate">
            <Setter.Value>
                <ControlTemplate>
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                        </Grid.ColumnDefinitions>

                        <nixxis:NixxisDataGrid Grid.Column="0" Grid.Row="0"
                                               Name="MainDataGrid"                                       
                                               CanUserAddRows="False" 
                                               CanUserDeleteRows="False"
                                               HeadersVisibility="Column"
                                               ItemsSource="{Binding}"
                                               ColumnsSelector="{Binding ElementName=ColumnSelector}"
                                               
                                               VirtualizingStackPanel.VirtualizationMode="Standard"
                                               RowDetailsVisibilityMode="Collapsed"
                                               SelectionUnit="CellOrRowHeader"
                                               IsReadOnly="True">

                            <nixxis:NixxisDataGrid.Columns>
                                <DataGridTemplateColumn Header="" CellStyle="{StaticResource BoolDataGridCellStyle}" >
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Image RenderOptions.BitmapScalingMode="HighQuality" Source="{Binding Type, Converter={StaticResource nixxisAlertIdIcon}}"></Image>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="{Binding Source={StaticResource tcxa}, Converter={StaticResource tcv}, ConverterParameter=Type}">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding Type}" Foreground="{Binding UnRead, Converter={StaticResource nixxisAlertUnReadForeColor}}"></TextBlock>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="{Binding Source={StaticResource tcxa}, Converter={StaticResource tcv}, ConverterParameter=UnRead}" CellStyle="{StaticResource BoolDataGridCellStyle}">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding UnRead}" Foreground="{Binding UnRead, Converter={StaticResource nixxisAlertUnReadForeColor}}"></TextBlock>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="{Binding Source={StaticResource tcxa}, Converter={StaticResource tcv}, ConverterParameter=Datetime}" CellStyle="{StaticResource TimeSpaneDataGridCellStyle}">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding DateTime, Converter={StaticResource dataGridFormatCommonConverter}}" Foreground="{Binding UnRead, Converter={StaticResource nixxisAlertUnReadForeColor}}"></TextBlock>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="{Binding Source={StaticResource tcxa}, Converter={StaticResource tcv}, ConverterParameter=Originator}">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding Originator}" Foreground="{Binding UnRead, Converter={StaticResource nixxisAlertUnReadForeColor}}"></TextBlock>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>                                
                                <DataGridTemplateColumn Header="{Binding Source={StaticResource tcxa}, Converter={StaticResource tcv}, ConverterParameter=Message}">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding Message}" Foreground="{Binding UnRead, Converter={StaticResource nixxisAlertUnReadForeColor}}"></TextBlock>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <!--<DataGridTextColumn Header="{Binding Source={StaticResource tcxa}, Converter={StaticResource tcv}, ConverterParameter=Type}"           Binding="{Binding Type}"                CellStyle="{StaticResource NumberColumnHeaderStyle}"/>
                                <DataGridTextColumn Header="{Binding Source={StaticResource tcxa}, Converter={StaticResource tcv}, ConverterParameter=UnRead}"         Foreground="{Binding UnRead, Converter={StaticResource nixxisAlertUnReadForeColor}}"   Binding="{Binding UnRead}"              CellStyle="{StaticResource BoolDataGridCellStyle}"/>
                                <DataGridTextColumn Header="{Binding Source={StaticResource tcxa}, Converter={StaticResource tcv}, ConverterParameter=Description}"    Foreground="{Binding UnRead, Converter={StaticResource nixxisAlertUnReadForeColor}}"   Binding="{Binding Description}"/>
                                <DataGridTextColumn Header="{Binding Source={StaticResource tcxa}, Converter={StaticResource tcv}, ConverterParameter=Datetime}"      Foreground="{Binding UnRead, Converter={StaticResource nixxisAlertUnReadForeColor}}"   Binding="{Binding DateTime, Converter={StaticResource dataGridFormatCommonConverter}}"        CellStyle="{StaticResource TimeSpaneDataGridCellStyle}"/>
                                <DataGridTextColumn Header="{Binding Source={StaticResource tcxa}, Converter={StaticResource tcv}, ConverterParameter=Message}"        Foreground="{Binding UnRead, Converter={StaticResource nixxisAlertUnReadForeColor}}"   Binding="{Binding Message}"/>-->
                            </nixxis:NixxisDataGrid.Columns>
                        </nixxis:NixxisDataGrid>

                        <ListBox Grid.Column="1" Grid.Row="0" Grid.RowSpan="2" Name="ColumnSelector" Visibility="Collapsed"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <!--Medium Template-->
        <Setter Property="MediumTemplate">
            <Setter.Value>
                <ControlTemplate>
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                        </Grid.ColumnDefinitions>

                        <nixxis:NixxisDataGrid Grid.Column="0" Grid.Row="0"
                                               Name="MainDataGridMedium"                                       
                                               CanUserAddRows="False" 
