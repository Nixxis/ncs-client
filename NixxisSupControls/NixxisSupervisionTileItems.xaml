                                                <n:NixxisChart.ChartPieces>
                                                    <n:NixxisChartPiece Color="{StaticResource SupPercent}" BindingValue="PeriodProduction.RationNeg_Arg" Label="{Binding Source={StaticResource tcxc}, Converter={StaticResource tcv}, ConverterParameter='Neg/Arg'}"></n:NixxisChartPiece >
                                                </n:NixxisChart.ChartPieces>
                                            </n:NixxisChart>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTextColumn Header="{Binding Source={StaticResource tcxc}, Converter={StaticResource tcv}, ConverterParameter=AvgPosValue_H}"                Binding="{Binding PeriodProduction.AvgPosValue}"                                                                          Visibility="Hidden" CellStyle="{StaticResource NumberColumnHeaderStyle}" />
                                <DataGridTextColumn Header="{Binding Source={StaticResource tcxc}, Converter={StaticResource tcv}, ConverterParameter=AvgNegValue_H}"                Binding="{Binding PeriodProduction.AvgNegValue}"                                                                          Visibility="Hidden" CellStyle="{StaticResource NumberColumnHeaderStyle}" />
                                <!--<DataGridTextColumn Header="{Binding Source={StaticResource tcxc}, Converter={StaticResource tcv}, ConverterParameter=sys_Reset_H}" Visibility="Hidden" Binding="{Binding PeriodProduction.sys_Reset}"/>-->
                            </nixxis:NixxisDataGrid.Columns>
                        </nixxis:NixxisDataGrid>

                        <ListBox Grid.Column="1" Grid.Row="0" Grid.RowSpan="2" Name="ColumnSelector" Visibility="Collapsed"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <!--Medium Template-->
        <Setter Property="MediumTemplate">
            <Setter.Value>
                <ControlTemplate>
                    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="#0E0E0E">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>

                        <ToolkitChart:Chart Grid.Row="0" Grid.Column="0" 
                                            Title="{Binding Source={StaticResource tcxc}, Converter={StaticResource tcv}, ConverterParameter='Contact list info'}"
                                            VerticalAlignment="Stretch" HorizontalAlignment="Stretch"
                                            Style="{StaticResource ToolkitChartColumnGlossVerStyle}"
                                            x:Name="graphContactListInfo">
                            <ToolkitChart:Chart.Axes>
                                <ToolkitChart:CategoryAxis Orientation="Y" 
                                                           ShowGridLines="True">
                                </ToolkitChart:CategoryAxis>
                                <ToolkitChart:LinearAxis Orientation="X"
                                                         ShowGridLines="False"
                                                         Visibility="Collapsed">

                                </ToolkitChart:LinearAxis>
                            </ToolkitChart:Chart.Axes>
                            <ToolkitChart:Chart.Series>
                                <ToolkitChart:ColumnSeries IsSelectionEnabled="False" 
                                                           ItemsSource="{Binding CampaignTotal.ContactListInfo.ContactInfoGraph}"
                                                           IndependentValueBinding="{Binding Path=Name}"
                                                           DependentValueBinding="{Binding Path=Value}">
                                </ToolkitChart:ColumnSeries>
                            </ToolkitChart:Chart.Series>
                        </ToolkitChart:Chart>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <!--Small Template-->
        <Setter Property="SmallTemplate">
            <Setter.Value>
                <ControlTemplate>
                    <ToolkitChart:Chart Style="{StaticResource ToolkitChartPieContactListInfoStyle}"
                                        VerticalAlignment="Stretch" HorizontalAlignment="Stretch"
                                        x:Name="mcChart">
                        <ToolkitChart:Chart.Series>
                            <ToolkitChart:PieSeries IsSelectionEnabled="False" Margin="0" 
                                                    ItemsSource="{Binding CampaignTotal.ContactListInfo.ContactInfoGraph}"  
                                                    IndependentValueBinding="{Binding Path=Name}" 
                                                    DependentValueBinding="{Binding Path=Value}">
                            </ToolkitChart:PieSeries>
                        </ToolkitChart:Chart.Series>
                    </ToolkitChart:Chart>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

