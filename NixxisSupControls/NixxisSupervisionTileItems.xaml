                            <nixxis:NixxisDataGrid.Columns>
                                <DataGridTemplateColumn Header="" CellStyle="{StaticResource BoolDataGridCellStyle}" >
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Image RenderOptions.BitmapScalingMode="HighQuality" Source="{Binding Type, Converter={StaticResource nixxisAlertIdIcon}}"></Image>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="{Binding Source={StaticResource tcxa}, Converter={StaticResource tcv}, ConverterParameter=Type}">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding Type}" Foreground="{Binding UnRead, Converter={StaticResource nixxisAlertUnReadForeColor}}"></TextBlock>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="{Binding Source={StaticResource tcxa}, Converter={StaticResource tcv}, ConverterParameter=UnRead}" CellStyle="{StaticResource BoolDataGridCellStyle}">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding UnRead}" Foreground="{Binding UnRead, Converter={StaticResource nixxisAlertUnReadForeColor}}"></TextBlock>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="{Binding Source={StaticResource tcxa}, Converter={StaticResource tcv}, ConverterParameter=Datetime}" CellStyle="{StaticResource TimeSpaneDataGridCellStyle}">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding DateTime, Converter={StaticResource dataGridFormatCommonConverter}}" Foreground="{Binding UnRead, Converter={StaticResource nixxisAlertUnReadForeColor}}"></TextBlock>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="{Binding Source={StaticResource tcxa}, Converter={StaticResource tcv}, ConverterParameter=Originator}">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding Originator}" Foreground="{Binding UnRead, Converter={StaticResource nixxisAlertUnReadForeColor}}"></TextBlock>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="{Binding Source={StaticResource tcxa}, Converter={StaticResource tcv}, ConverterParameter=Message}">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding Message}" Foreground="{Binding UnRead, Converter={StaticResource nixxisAlertUnReadForeColor}}"></TextBlock>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                            </nixxis:NixxisDataGrid.Columns>
                        </nixxis:NixxisDataGrid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <!--Small Template-->
        <Setter Property="SmallTemplate">
            <Setter.Value>
                <ControlTemplate>
                    <StackPanel Orientation="Vertical" 
                                VerticalAlignment="Stretch"
                                HorizontalAlignment="Stretch" 
                                Background="#0E0E0E">
                        <TextBlock Foreground="Red"
                                   TextAlignment="Center" 
                                   Text="Unread" 
                                   FontSize="16"></TextBlock>
                        <TextBlock Foreground="Red"
                                   TextAlignment="Center" 
                                   Text="{Binding UnReadItems}" 
                                   FontSize="24"></TextBlock>       
                        <TextBlock Foreground="Snow"
                                   TextAlignment="Center" 
                                   Text="Message count" 
                                   FontSize="16"></TextBlock>
                        <TextBlock Foreground="White"
                                   TextAlignment="Center" 
                                   Text="{Binding Count}" 
                                   FontSize="20"></TextBlock>
                 
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

