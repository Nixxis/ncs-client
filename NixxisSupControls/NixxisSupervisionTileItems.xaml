    <!--Column Chart-->
    <!--___________ -->
    <!--ColumnDataPoint Template High gloss Horizontal-->
    <ControlTemplate x:Key="DefaultColumnDataPointGlossHorControlTemplate" TargetType="ToolkitChart:ColumnDataPoint">
        <Border BorderThickness="0" Opacity="1" x:Name="Root">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualStateGroup.Transitions>
                        <VisualTransition GeneratedDuration="0:0:0.1" />
                    </VisualStateGroup.Transitions>
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="MouseOverHighlight" Storyboard.TargetProperty="Opacity" To="0.6" Duration="0" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="SelectionStates">
                    <VisualStateGroup.Transitions>
                        <VisualTransition GeneratedDuration="0:0:0.1" />
                    </VisualStateGroup.Transitions>
                    <VisualState x:Name="Unselected" />
                    <VisualState x:Name="Selected">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="SelectionHighlight" Storyboard.TargetProperty="Opacity" To="0.6" Duration="0" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="RevealStates">
                    <VisualStateGroup.Transitions>
                        <VisualTransition GeneratedDuration="0:0:0.5" />
                    </VisualStateGroup.Transitions>
                    <VisualState x:Name="Shown">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="Root" Storyboard.TargetProperty="Opacity" To="1" Duration="0" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Hidden">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="Root" Storyboard.TargetProperty="Opacity" To="0" Duration="0" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>

            <Grid Margin="0 4 0 4">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>

                <!-- Main bar shape -->
                <Rectangle Fill="{Binding Color, Converter={StaticResource htmlColorCodeToSolidBrushConverter}}"
                           Stroke="{TemplateBinding BorderBrush}"
                           StrokeThickness="{TemplateBinding BorderThickness}"
                           RadiusX="3"
                           RadiusY="3"
                           Grid.Row="0"
                           Grid.RowSpan="2">
                    <Rectangle.Effect>
                        <DropShadowEffect BlurRadius="10" Direction="0" Color="#FFFFFFFF" ShadowDepth="0" />
                    </Rectangle.Effect>
                </Rectangle>


                <!-- Diffuse Glow -->
                <Rectangle RadiusX="3" RadiusY="3" Margin="4" Opacity=".25" Fill="White"
                           Grid.Row="0" Grid.RowSpan="2">
                    <Rectangle.Effect>
                        <BlurEffect Radius="8" />
                    </Rectangle.Effect>
                </Rectangle>

                <!-- Specular Highlight -->
                <Rectangle RadiusX="3" RadiusY="3" Margin="2" Grid.Row="0">
                    <Rectangle.Fill>
                        <LinearGradientBrush>
                            <GradientStop Color="#99ffffff" Offset="0" />
                            <GradientStop Color="#22ffffff" Offset="1" />
                        </LinearGradientBrush>
                    </Rectangle.Fill>
                </Rectangle>

                <Rectangle x:Name="SelectionHighlight" Fill="Red" Opacity="0" />
                <Rectangle x:Name="MouseOverHighlight" RadiusX="3" RadiusY="3" Fill="White" Opacity="0" />
            </Grid>
            <ToolTipService.ToolTip>
                <StackPanel>
                    <ContentControl Content="{TemplateBinding IndependentValue}" FontWeight="Bold"/>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="{Binding Source={x:Static crs:TranslationContext.Default}, Converter={x:Static nixxis:TranslationConverter.Default}, ConverterParameter='Value:'}"></TextBlock>
                        <ContentControl Content="{TemplateBinding FormattedDependentValue}"/>
                    </StackPanel>
                </StackPanel>
            </ToolTipService.ToolTip>
        </Border>
    </ControlTemplate>
